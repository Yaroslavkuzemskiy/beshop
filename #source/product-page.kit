<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css" />
    <title>BeShop</title>
</head>
<body>
    <div class="wrapper">
        <!-- @import "header.html"-->
        <main>
            <section class="hero-small" style="background-image: url('assets/bg.png')";>
                <div class="container-middle">
                    <div class="hero-small-main">
                        <h1>Shop</h1>
                        <nav class="breadcrumbs">
                            <span>Home</span>
                            -
                            <span class="span-active">Shop</span>
                        </nav>
                    </div>
                    
                </div>
            </section>
            <section class="product-description">
                <div class="container-logos">
                    <div class="product-grid">
                        <div class="product-details">
                            <div class="product-detail-images">
    
                                <div style="--swiper-navigation-color: #fff; --swiper-pagination-color: #fff" class="swiper mySwiper2">
                                    <div class="swiper-wrapper">
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-1.jpg" />
                                            <div class="image-lable">sale</div>
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-2.jpg" />
                                            <div class="image-lable">sale</div>
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-3.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-4.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-5.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-6.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-7.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-8.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-9.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-10.jpg" />
                                        </div>
                                    </div>
                                    <div class="swiper-button-next"></div>
                                    <div class="swiper-button-prev"></div>
                                </div>
                                <div thumbsSlider="" class="swiper mySwiper">
                                    <div class="swiper-wrapper">
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-1.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-2.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-3.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-4.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-5.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-6.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-7.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-8.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-9.jpg" />
                                        </div>
                                        <div class="swiper-slide slide-product">
                                            <img src="https://swiperjs.com/demos/images/nature-10.jpg" />
                                        </div>
                                    </div>
                                </div>
                              
                              
                                
    
                            </div>
                            
                        </div>
                        <div class="product-detail-description">
                            <div class="description-title">Foundation Beshop</div>
                            <div class="description-sku">
                                <div class="sku-stock">in stock</div>
                                <div class="sku">sku: ini203</div>
                            </div>
                            <div class="description-price cart-price">
                                <div class="price-old old-price">$222</div>
                                <div class="price-new new-price">$100</div>
                            </div>
                            <div class="description-text">
                                Universal foundation masks skin imperfections and gives it a matte finish, remaining completely invisible on the face. The foundation is suitable for any skin type. Thanks to a unique combination of pigments, the foundation adapts perfectly to even the smallest features of skin tone, creating an even and natural tone. 
                            </div>
                            <div class="description-socials">
                                <div class="find-descroption description-find-socials">Find us here:</div>
                                <div class="footer-icons">
                                    <div class="icon-logo description-find-logo">
                                        <a href="#" class="find-logo-link">
                                            <img src="assets/icons/fb.png" alt="" class="find-logo-social">
                                        </a>
                                    </div>
                                    <div class="icon-logo description-find-logo">
                                        <a href="#" class="find-logo-link">
                                            <img src="assets/icons/tw.png" alt="" class="find-logo-social">
                                        </a>
                                    </div>
                                    <div class="icon-logo description-find-logo">
                                        <a href="#" class="find-logo-link">
                                            <img src="assets/icons/inst.png" alt="" class="find-logo-social">
                                        </a>
                                    </div>
                                    <div class="icon-logo description-find-logo">
                                        <a href="#" class="find-logo-link">
                                            <img src="assets/icons/linke.png" alt="" class="find-logo-social">
                                        </a>
                                    </div>
                                </div>

                                

                            </div>
                            <hr>
                            <div class="description-menu">
                                <div class="description-menu-color">
                                    <div class="color-title">Color</div>
                                    <div class="color-items">
                                        <label for="color-red">
                                            <input type="radio" id="color-red" class="color" name="color">
                                            <span class="color-red"></span>
                                        
                                        </label>
                                        <label for="color-green">
                                            <input type="radio" id="color-green" class="color" name="color">
                                            <span class="color-green"></span>
                                        
                                        </label>
                                        <label for="color-blue">
                                            <input type="radio" id="color-blue" class="color" name="color">
                                            <span class="color-blue"></span>
                                        
                                        </label>
                                        
                                    </div>
                                    
                                </div>
                                <div class="description-menu-count">
                                    <div class="count-title">Quantity</div>
                                    <div class="count-input">
                                        <input class="count-field" type="number" value="1">
                                    </div>
                                </div>
                            </div>
                            <div class="description-btns">
                                <a href="#" class="btn">Cart</a>
                                <a href="#" class="btn grey">Wish</a>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </section>
        </main>
        <!-- @import "footer.html"-->
        
